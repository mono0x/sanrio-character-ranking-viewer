<%! func renderLayout(w io.Writer, title string, render func(io.Writer) error) error %>
<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title><%= title %></title>
    <link rel="stylesheet" type="text/css" href="/assets/app.css" />
    <script type="text/javascript" src="/assets/app.js" async></script>
  </head>
  <body>
    <div class="container">
      <% _ = render(w) %>
    </div>
  </body>
</html>
